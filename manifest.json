{
    "name": "Original Toots Only",
    "description": "A chrome extension that let you view only original toots in other people's profile",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/logo.png"
          }
      },
      "icons": {
        "128": "/images/logo.png"
      }
  }